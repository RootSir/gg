function Fan_searchNumber(n, type, ft, sign, r, s)
  gg.setVisible(false)
  gg.searchNumber(n, type, ft, sign, r, s)
  if gg.isVisible(true) then
    gg.setVisible(false)
    gg.processKill()
    print("请勿窥码！谢谢！！")
    gg.toast("")
    while true do
      gg.setVisible(false)
    end
  end
end

while true do
  if gg.isVisible(true) then
    XGCK = 1
    gg.setVisible(false)
  end
  gg.clearResults()
  if XGCK == 1 then
    BT()
  end
end

function TK()
  TK = gg.choice({
    '1.黑夜天空',
    '2.蔚蓝天空',
    '3.晨青天空',
    '4.粉甜天空',
    '5.全图除草',
    '6.返回首页'
  }, nil, '提示:在出生岛开启')
if TK == nil then else
  if TK == 1 then
    TK1()
  end
  if TK == 2 then
    TK2()
  end
  if TK == 3 then
    TK3()
  end
  if TK == 4 then
    TK4()
  end
  if TK == 5 then
    TK10()
  end
  if TK == 6 then
    Main() 
  end
  XGCK=-1
end
end


function TK1()
  gg.clearResults()
  gg.setRanges(gg.REGION_BAD)
  gg.searchNumber("403,200.0;4,718.40625;100", gg.TYPE_FLOAT, false, gg.SING_EQUAL, 0, -1)
  gg.searchNumber("100", gg.TYPE_FLOAT, false, gg.SING_EQUAL, 0, -1)
  gg.getResults(100)
  gg.editAll("-90", gg.TYPE_FLOAT)
  gg.toast("黑夜天空开启成功  仅海岛有效")
  gg.clearResults()
end

